{
    "name": "@nivinjoseph/n-app",
    "version": "4.0.37",
    "description": "Vue.js based application framework",
    "packageManager": "yarn@4.0.2",
    "type": "module",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./plugins": {
            "type": "./dist/plugins/index.d.ts",
            "node": "./dist/plugins/index.js",
            "default": null
        }
    },
    "scripts": {
        "ts-compile": "tsc -p tsconfig.json",
        "ts-lint": "eslint . --ext .ts",
        "ts-compile-test-app-server": "tsc -p ./tsconfig.server.json",
        "ts-compile-test-app-client": "tsc -p ./tsconfig.client.json",
        "ts-compile-test-app-server-watch": "yarn ts-compile-test-app-server -w --preserveWatchOutput",
        "ts-build": "yarn ts-compile && yarn ts-lint",
        "ts-build-dist": "yarn ts-build && tsc -p ./dist",
        "clean-src": "find ./src -name '*.js' -delete -o -name '*.map' -delete && yarn clean-build-info && rm -f *.tsbuildinfo",
        "clean-test": "find ./test -name '*.js' -delete -o -name '*.map' -delete && rm -f *.tsbuildinfo",
        "clean-test-app": "find ./test-app -name '*.js' -delete -o -name '*.map' -delete  -o -name '*d.ts' -delete && rm -f *.tsbuildinfo",
        "clean-dist": "find ./dist -name '*.js' -delete -o -name '*.map' -delete -o -name '*.ts' -o -name '*.tsbuildinfo' -delete",
        "clean-build-info": "find . -type f -name '*.tsbuildinfo' -delete",
        "clean-all": "yarn clean-src && yarn clean-test && yarn clean-test-app && yarn clean-dist",
        "test": "yarn ts-build && node --test --enable-source-maps ./test",
        "publish-package": "yarn ts-build-dist && git add . && git commit -m 'preparing to publish new version' && yarn version patch && git push && yarn npm publish --access=public",
        "dev-test-app-server": "yarn ts-compile-test-app-server && concurrently -p \"[{name}]\" -n \"TS,API\" -c \"red.bold,magenta.bold\" \"yarn:ts-compile-test-app-server-watch\" \"node --watch --inspect=4001 --enable-source-maps ./test-app/server.js env=dev\"",
        "dev-test-app-client": "vite --config ./vite.config.ts",
        "dev": "concurrently -p \"[{name}]\"  -n \"SERVER,APP\" -c \"blue.bold,green.bold\" \"yarn:dev-test-app-server\" \"yarn:dev-test-app-client\"",
        "build-test-app-client": "env=prod vite build --config ./vite.config.ts",
        "build-test-app-server": "env=prod yarn ts-compile-test-app-server",
        "build-test-app": "yarn build-test-app-server && yarn build-test-app-client",
        "start-test-app": "node --inspect=4001 --enable-source-maps ./test-app/server.js env=prod",
        "detect-circular-dependencies": "madge --circular --extensions ts ./"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/nivinjoseph/n-app.git"
    },
    "keywords": [
        "SPA",
        "Vue.js",
        "Framework"
    ],
    "author": "NiviN",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/nivinjoseph/n-app/issues"
    },
    "homepage": "https://github.com/nivinjoseph/n-app#readme",
    "peerDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/plugin-proposal-decorators": "^7.24.7",
        "@babel/plugin-transform-class-properties": "^7.24.7",
        "@babel/plugin-transform-runtime": "^7.23.7",
        "@babel/preset-env": "^7.25.3",
        "@babel/runtime": "^7.25.0",
        "@vue/compiler-sfc": "^3.4.36",
        "autoprefixer": "^10.4.20",
        "sass": "^1.77.8",
        "vite": "^5.3.5",
        "vite-plugin-checker": "^0.7.2",
        "vite-plugin-inspect": "^0.8.5",
        "vite-plugin-require": "^1.2.14",
        "vite-plugin-top-level-await": "^1.4.2"
    },
    "devDependencies": {
        "@babel/core": "^7.25.2",
        "@babel/plugin-proposal-decorators": "^7.24.7",
        "@babel/plugin-transform-class-properties": "^7.24.7",
        "@babel/plugin-transform-runtime": "^7.23.7",
        "@babel/preset-env": "^7.25.3",
        "@babel/runtime": "^7.25.0",
        "@nivinjoseph/n-sock": "^2.0.2",
        "@nivinjoseph/n-web": "https://github.com/shreymahendru/n-web#ts-5-build",
        "@rollup/plugin-inject": "^5.0.5",
        "@types/babel__core": "^7",
        "@types/babel__plugin-transform-runtime": "^7",
        "@types/babel__preset-env": "^7",
        "@types/jquery": "^3.5.29",
        "@types/node": "^20.10",
        "@types/toastr": "^2.1.39",
        "@typescript-eslint/eslint-plugin": "^6.15.0",
        "@typescript-eslint/parser": "^6.15.0",
        "@vue/compiler-sfc": "^3.4.36",
        "autoprefixer": "^10.4.20",
        "concurrently": "^8.2.2",
        "eslint": "^8.56.0",
        "eslint-plugin-require-extensions": "^0.1.3",
        "feather-icons": "^4.29.1",
        "madge": "^6.1.0",
        "postcss": "^8.4.41",
        "sass": "^1.77.8",
        "terser": "^5.27.0",
        "typescript": "^5.3.3",
        "vite": "^5.3.5",
        "vite-plugin-checker": "^0.7.2",
        "vite-plugin-inspect": "^0.8.5",
        "vite-plugin-node-polyfills": "^0.22.0",
        "vite-plugin-require": "^1.2.14",
        "vite-plugin-top-level-await": "^1.4.2"
    },
    "dependencies": {
        "@nivinjoseph/n-config": "^2.0.1",
        "@nivinjoseph/n-defensive": "^2.0.1",
        "@nivinjoseph/n-exception": "^2.0.1",
        "@nivinjoseph/n-ext": "^2.0.1",
        "@nivinjoseph/n-ject": "^3.0.2",
        "@nivinjoseph/n-util": "^2.0.1",
        "jquery": "^3.7.1",
        "simplebar": "^5.3.6",
        "vue": "3.3.11",
        "vue-router": "4.2.5"
    },
    "engineStrict": true,
    "engines": {
        "node": ">=20.10"
    },
    "browserslist": [
        "defaults"
    ],
    "madge": {
        "detectiveOptions": {
            "ts": {
                "skipTypeImports": true
            }
        }
    }
}
